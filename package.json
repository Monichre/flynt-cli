{
  "name": "flynt-cli",
  "version": "0.0.1",
  "description": "CLI tool for the WordPress Flynt framework",
  "main": "src/index.js",
  "repository": "https://github.com/bleech/flynt-cli.git",
  "author": "bleech",
  "license": "MIT",
  "scripts": {
    "build": "npm run lint && npm run clean && babel src -d lib",
    "watch": "npm run clean && babel src --watch -d lib",
    "clean": "rm -rf ./lib",
    "lint": "./node_modules/standard/bin/cmd.js --parser babel-eslint ./src/**/*.js"
  },
  "bin": {
    "flynt": "lib/index.js"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-preset-env": "^1.5.1",
    "babel-preset-stage-2": "^6.22.0",
    "eslint": "^3.12.1",
    "standard": "^8.6.0"
  },
  "dependencies": {
    "bluebird": "^3.4.6",
    "hasbin": "^1.2.3",
    "inquirer": "^1.2.3",
    "lodash": "^4.17.2",
    "replace-in-file": "^2.2.2",
    "search-replace-db": "interconnectit/Search-Replace-DB",
    "yargs": "^6.5.0"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "/lib/"
    ]
  },
  "phpDependencies": {
    "roots/bedrock": "1.7.7",
    "timber/timber": "1.2.4",
    "flyntwp/flynt-core": "1.0.0",
    "flyntwp/acf-field-group-composer": "1.0.0",
    "composer/installers": "^1.0",
    "philippbaschke/acf-pro-installer": "^1.0",
    "igniteonline/wpm-pro-installer": "^1.0.2",
    "advanced-custom-fields/advanced-custom-fields-pro": "5.5.12",
    "deliciousbrains/wp-migrate-db-pro": "1.7.1",
    "deliciousbrains/wp-migrate-db-pro-media-files": "1.4.7"
  }
}
